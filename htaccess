php_value  error_reporting 2047

RewriteEngine On

# Block User-Agent: 
# 1-ый вариант RewriteCond %{HTTP_USER_AGENT} "Chrome/92.0.4476.0 Safari/537.36" [NC] - проверка на содержание куска (скобки нельзя чтоб были, только слова, цифры, точка, слеш)
# 2-ой вариант RewriteCond %{HTTP_USER_AGENT} "=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/92.0.4476.0 Safari/537.36" [NC] - проверка на соответствие полностью всего юзер агента (перед параметром ставится '=')
# После каждого ставится флаг [NC,OR], если редирект I типа то у последнего [NC], если II то [NC,OR]
RewriteCond %{HTTP_USER_AGENT} "Mozilla/4.0" [NC,OR]
